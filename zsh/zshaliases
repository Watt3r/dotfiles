alias python=python3
alias pip=pip3

alias proxy=~/scripts/proxy.sh
alias noproxy=~/scripts/noproxy.sh

alias ctags='/usr/local/bin/ctags'

alias notif='terminal-notifier -ignoreDnD -title "Terminal"'

# Git aliases
alias g='git'
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gcm='git commit -m'

export TERM=xterm-256color
